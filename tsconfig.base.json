{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "nodenext",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2021",
    "module": "esnext",
    "lib": ["es2021", "dom", "dom.iterable"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "strictNullChecks": false,
    "strictFunctionTypes": false,
    "strictPropertyInitialization": false,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "paths": {
      "@geonetwork-ui/api/metadata-converter": [
        "libs/api/metadata-converter/src/index.js"
      ],
      "@geonetwork-ui/api/repository": ["libs/api/repository/src/index.js"],
      "@geonetwork-ui/api/repository/gn4": [
        "libs/api/repository/src/lib/gn4/index.js"
      ],
      "@geonetwork-ui/common/domain/*": ["libs/common/domain/src/lib/*"],
      "@geonetwork-ui/common/fixtures": ["libs/common/fixtures/src/index.js"],
      "@geonetwork-ui/data-access/gn4": ["libs/data-access/gn4/src/index.js"],
      "@geonetwork-ui/data-fetcher": ["libs/util/data-fetcher/src/index.js"],
      "@geonetwork-ui/feature/catalog": ["libs/feature/catalog/src/index.js"],
      "@geonetwork-ui/feature/dataviz": ["libs/feature/dataviz/src/index.js"],
      "@geonetwork-ui/feature/editor": ["libs/feature/editor/src/index.js"],
      "@geonetwork-ui/feature/map": ["libs/feature/map/src/index.js"],
      "@geonetwork-ui/feature/notifications": [
        "libs/feature/notifications/src/index.js"
      ],
      "@geonetwork-ui/feature/record": ["libs/feature/record/src/index.js"],
      "@geonetwork-ui/feature/router": ["libs/feature/router/src/index.js"],
      "@geonetwork-ui/feature/search": ["libs/feature/search/src/index.js"],
      "@geonetwork-ui/ui/catalog": ["libs/ui/catalog/src/index.js"],
      "@geonetwork-ui/ui/dataviz": ["libs/ui/dataviz/src/index.js"],
      "@geonetwork-ui/ui/elements": ["libs/ui/elements/src/index.js"],
      "@geonetwork-ui/ui/inputs": ["libs/ui/inputs/src/index.js"],
      "@geonetwork-ui/ui/layout": ["libs/ui/layout/src/index.js"],
      "@geonetwork-ui/ui/libs/copy-text-button": [
        "libs/ui/inputs/src/lib/copy-text-button/copy-text-button.component.js"
      ],
      "@geonetwork-ui/ui/map": ["libs/ui/map/src/index.js"],
      "@geonetwork-ui/ui/search": ["libs/ui/search/src/index.js"],
      "@geonetwork-ui/ui/widgets": ["libs/ui/widgets/src/index.js"],
      "@geonetwork-ui/util/app-config": ["libs/util/app-config/src/index.js"],
      "@geonetwork-ui/util/i18n": ["libs/util/i18n/src/index.js"],
      "@geonetwork-ui/util/i18n/date-locales": [
        "libs/util/i18n/src/lib/date-locales.js"
      ],
      "@geonetwork-ui/util/i18n/language-codes": [
        "libs/util/i18n/src/lib/language-codes.js"
      ],
      "@geonetwork-ui/util/shared": ["libs/util/shared/src/index.js"]
    },
    "allowJs": true,
    "noEmit": true,
    "outDir": "/"
  },
  "exclude": ["node_modules", "tmp"]
}
