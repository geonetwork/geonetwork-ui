  <skos:Concept rdf:about="https://data.vlaanderen.be/id/concept/ontwikkelingstoestand/BETA">
    <skos:inscheme rdf:resource="https://data.vlaanderen.be/id/conceptscheme/ontwikkelingstoestand"/>
    <skos:topConceptOf rdf:resource="https://data.vlaanderen.be/id/conceptscheme/ontwikkelingstoestand"/>
    <skos:definition xml:lang="nl">Testomgeving voor integratietesten met andere teams/partijen. Bijvoorbeeld: end-to-end testen van componenten die reeds in hun eigen testomgeving getest zijn met mock-data/interfaces.</skos:definition>
    <skos:prefLabel xml:lang="nl">beta</skos:prefLabel>
    <skos:prefLabel xml:lang="en">beta</skos:prefLabel>
    <skos:prefLabel xml:lang="fr">beta</skos:prefLabel>
    <skos:prefLabel xml:lang="de">beta</skos:prefLabel>
  </skos:Concept>

  <skos:Concept rdf:about="https://data.vlaanderen.be/id/concept/ontwikkelingstoestand/TNI">
    <skos:inscheme rdf:resource="https://data.vlaanderen.be/id/conceptscheme/ontwikkelingstoestand"/>
    <skos:topConceptOf rdf:resource="https://data.vlaanderen.be/id/conceptscheme/ontwikkelingstoestand"/>
    <skos:definition xml:lang="nl">Stabiele omgeving, waar code gedeployed wordt NA succesvolle integratie testen op de beta-omgeving. Kan dienst doen als demo-omgeving, indien meerdere partijen met elkaar moeten integreren.</skos:definition>
    <skos:prefLabel xml:lang="nl">tni</skos:prefLabel>
    <skos:prefLabel xml:lang="en">tni</skos:prefLabel>
    <skos:prefLabel xml:lang="fr">tni</skos:prefLabel>
    <skos:prefLabel xml:lang="de">tni</skos:prefLabel>
  </skos:Concept>

