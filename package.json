  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "start": "nx serve datahub",
    "build": "nx affected:build",
    "lint": "nx affected:lint",
    "test": "TEST_HIDE_CONSOLE=1 nx affected:test",
    "apps": "nx affected:apps",
    "libs": "nx affected:libs",
    "lint:all": "nx run-many --all --target=lint",
    "test:all": "TEST_HIDE_CONSOLE=1 nx run-many --all --target=test",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "storybook": "nx run demo:storybook",
    "build:storybook": "nx run demo:build-storybook",
    "storybook-wc": "nx run webcomponents:build --output-hashing=none && nx run webcomponents:storybook",
    "build:storybook-wc": "nx run webcomponents:build --output-hashing=none && nx run webcomponents:build-storybook",
    "build:demo": "tools/webcomponent/prepare-wc-pages.sh",
    "demo": "tools/webcomponent/prepare-wc-pages.sh --serve",
    "demo:local": "tools/webcomponent/prepare-wc-pages.sh --serve --local",
    "webcomponents:e2e": "npx cypress open --project apps/webcomponents --e2e --browser chrome",
    "webcomponents:e2e:ci": "npm run demo:local & wait-on http://localhost:8001 && npx cypress run --project apps/webcomponents",
    "generate-api": "tools/generate-api.sh",
    "dev:ssr": "ng run formatter:serve-ssr",
    "serve:ssr": "node dist/formatter/server/main.js",
    "build:ssr": "nx build --configuration production && ng run formatter:server:production",
    "prerender": "nx run formatter:prerender",
    "i18n:extract": "ngx-translate-extract -s --fi '  ' --input ./apps ./libs --output ./translations/{en,de,fr,it,es,nl,pt,sk}.json --clean --format json && npm run format:write",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "version": "node tools/update-all-package-json.js && git add package"
