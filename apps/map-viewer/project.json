{
  "name": "map-viewer",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/map-viewer/src",
  "prefix": "map-viewer",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/map-viewer",
        "index": "apps/map-viewer/src/index.html",
        "main": "apps/map-viewer/src/main.ts",
        "polyfills": "apps/map-viewer/src/polyfills.ts",
        "tsConfig": "apps/map-viewer/tsconfig.app.json",
        "assets": [
          "apps/map-viewer/src/favicon.ico",
          "apps/map-viewer/src/assets",
          {
            "glob": "*",
            "input": "translations",
            "output": "assets/i18n/"
          }
        ],
        "styles": [
          "tailwind.base.css",
          "apps/map-viewer/src/styles.css",
          "node_modules/ol/ol.css",
          "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/map-viewer/src/environments/environment.ts",
              "with": "apps/map-viewer/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "map-viewer:build:production"
        },
        "development": {
          "browserTarget": "map-viewer:build:development",
          "proxyConfig": "proxy-config.js"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "map-viewer:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/map-viewer/src/**/*.ts",
          "apps/map-viewer/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/map-viewer"],
      "options": {
        "jestConfig": "apps/map-viewer/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["type:app"]
}
